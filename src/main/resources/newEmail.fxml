<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.projectuexcel.ui.NewEmailController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="400.0" prefWidth="400.0" VBox.vgrow="NEVER">
      <children>
          <Button fx:id="buttonAddEmail" layoutX="45.0" layoutY="290.0" mnemonicParsing="false" onAction="#addEmail" prefHeight="60.0" prefWidth="140.0" text="Add email" />
          <Button fx:id="buttonSkip" layoutX="222.0" layoutY="290.0" mnemonicParsing="false" onAction="#skip" prefHeight="60.0" prefWidth="140.0" text="Skip" />
          <TextField fx:id="email" layoutX="41.0" layoutY="107.0" promptText="Email" />
            <Text fx:id="textNotFound" layoutX="76.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Email was not found for teacher with code " />
      </children>
    </AnchorPane>
  </children>
</VBox>

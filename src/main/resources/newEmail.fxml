<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="231.0" prefWidth="400.0" stylesheets="@main.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.projectuexcel.ui.NewEmailController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="400.0" prefWidth="400.0" VBox.vgrow="NEVER">
      <children>
          <Button fx:id="buttonAddEmail" layoutX="42.0" layoutY="152.0" mnemonicParsing="false" onAction="#addEmail" prefHeight="25.0" prefWidth="140.0" text="Add email" />
          <Button fx:id="buttonSkip" layoutX="210.0" layoutY="152.0" mnemonicParsing="false" onAction="#skip" prefHeight="4.0" prefWidth="140.0" text="Skip" />
          <TextField fx:id="email" layoutX="42.0" layoutY="91.0" prefHeight="35.0" prefWidth="316.0" promptText="Email" />
            <Text fx:id="textNotFound" layoutX="76.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Email was not found for teacher with code " />
      </children>
    </AnchorPane>
  </children>
</VBox>
